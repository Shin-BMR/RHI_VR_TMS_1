<?xml version="1.0" encoding="utf-8"?>
<modules>
    <webcam>
        <id>webcam</id>
    </webcam>
    <video_see_through>
        <camera>
            <id>logitech.c920</id>
            <interface>
                <input>webcam</input>
                <output>video_see_through_image</output>
            </interface>
            <width>1920</width>
            <height>1080</height>
            <fov>60</fov>
        </camera>
    </video_see_through>
</modules>
